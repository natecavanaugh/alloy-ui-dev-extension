{
	"manifest_version": 2,
	"name": "AlloyUI Extension",
	"version": "1.0",

	"description": "Tools to help with working with AlloyUI on your webpage",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["javascript.js"]
		}
	],

	"permissions": [
		"experimental", "tabs", "http://*/*", "https://*/*"
	]
}